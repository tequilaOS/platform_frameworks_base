<resources>
    <!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Proximity check on screen on -->
    <java-symbol type="bool" name="config_proximityCheckOnWake" />

    <!-- Proximity check timeout -->
    <java-symbol type="integer" name="config_proximityCheckTimeout" />

    <!-- Proximity check on screen on default -->
    <java-symbol type="bool" name="config_proximityCheckOnWakeEnabledByDefault" />

    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- Camera -->
    <java-symbol type="array" name="config_cameraAuxPackageAllowList" />
    <java-symbol type="array" name="config_cameraAuxPackageExcludeList" />

    <!-- Cycle through ringer modes -->
    <java-symbol type="string" name="volume_dialog_ringer_guidance_normal" />

    <!-- Power Menu Reboot -->
    <java-symbol name="reboot_to_bootloader_title" type="string" />
    <java-symbol name="reboot_to_bootloader_message" type="string" />
    <java-symbol name="reboot_to_recovery_title" type="string" />
    <java-symbol name="reboot_to_recovery_message" type="string" />
    <java-symbol name="reboot_system_title" type="string" />
    <java-symbol name="reboot_system_message" type="string" />
    <java-symbol type="array" name="config_rebootActionsList" />

    <!-- Whether device has physical tri state switch -->
    <java-symbol type="bool" name="config_hasAlertSlider" />

    <!-- The location of the devices physical tri state switch -->
    <java-symbol type="integer" name="config_alertSliderLocation" />

    <!-- Enable face auth only when swiping security view -->
    <java-symbol type="bool" name="config_faceAuthOnlyOnSecurityView" />
    
    <!-- Package Manager -->
    <java-symbol type="array" name="config_deviceDisabledComponents" />
    <java-symbol type="array" name="config_globallyDisabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- Location indicator -->
    <java-symbol type="drawable" name="stat_sys_location" />
</resources>
